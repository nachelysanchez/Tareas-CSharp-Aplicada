        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MenuEjercicio4"
        mc:Ignorable="d"
        Title="Ventana Principal" Height="300" Width="400">
    <Grid>
        <StackPanel>
            <Menu Height="18" Margin="0,0,0,266">
